# Server Configuration
PORT=3001
NODE_ENV=development

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Kinesis Configuration
KINESIS_STREAM_NAME=resources-stream
ELIXIR_STREAM_NAME=elixir-notifications

# Citrus Sharding Configuration
CITRUS_SERVER_URL=http://localhost:8080
CITRUS_API_KEY=your-citrus-api-key
CITRUS_TIMEOUT=5000
CITRUS_RETRY_ATTEMPTS=3

# Validation Configuration
VALIDATION_ENABLED=true

# SQS Configuration for Shard Creation Notifications
# Create this queue manually or through your infrastructure management process
SQS_SHARD_CREATION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/shard-creation-queue 