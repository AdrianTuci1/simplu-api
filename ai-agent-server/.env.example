# Server Configuration
NODE_ENV=development
PORT=3000

# Database Configuration
EXTERNAL_DATABASE_URL=postgresql://user:password@postgres-external:5432/dbname

# Kafka Configuration
EXTERNAL_KAFKA_BROKERS=kafka-external:9092
KAFKA_CLIENT_ID=agent-server
KAFKA_GROUP_ID=agent-group

# OpenRouter Configuration
OPENROUTER_API_KEY=your_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone
TWILIO_WHATSAPP_NUMBER=your_twilio_whatsapp_number

# Email Configuration
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASSWORD=your_smtp_password
EMAIL_FROM=your_from_email

# IMAP Configuration
IMAP_HOST=your_imap_host
IMAP_PORT=993
IMAP_USER=your_imap_username
IMAP_PASSWORD=your_imap_password
